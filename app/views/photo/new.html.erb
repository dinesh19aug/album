<%= form_for @photo ,:url => { :action => "create" } do |f| %>
	<%= f.label :pic_name%>
	<%= f.text_field :pic_name%><br>

	<%= f.label :description%>
	<%= f.text_field :description%>
<%= select("photo", "albums_id", Albums.all.collect {|a| [ a.album_name, a.id ] }, { :include_blank => true }) %>
	
	<%= f.submit%>
<%end%>
